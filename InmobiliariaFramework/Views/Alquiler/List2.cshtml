@model IEnumerable<InmobiliariaFramework.Models.CarpetaRepo.Auxiliar>




<table class="table" border="solid 1px">
    <tr>
        <th colspan="4">
           Datos Alquiler
        </th>
        <th colspan="4">
           Datos Inquilino
        </th>
        <th  <th colspan="2">
           Datos Inmueble
        </th>
        <th colspan="4">
            Datos Propietario
        </th>
        <th></th>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdAlquiler)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_inicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_fin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdInquilino)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DniP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApellidoP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdInmueble)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdPropietario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DniP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApellidoP)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAlquiler)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_inicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_fin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInquilino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInmueble)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPropietario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DniP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApellidoP)
            </td>
            <td>
                @Html.ActionLink("Ver Pagos", "VerPagos", new { id = item.IdAlquiler })
                @Html.ActionLink("Renovar", "Edit", new { id = item.IdAlquiler })
                @Html.ActionLink("Rescindir", "Rescindir", new { id = item.IdAlquiler })
            </td>
        </tr>
    }

</table>
